      <Query>
        <DataSourceName>CPU</DataSourceName>
        <CommandText>/*
-- parameters used by report
ReportFilter, SortBy, ReportCurrency, PrintAllInBaseCurrency

-- data fields used by report
SortBy, SoldToId, BlanketId, BlanketRef, BlanketType, BlanketStatus, ShipToId, CloseDate, ExpireDate, Rep1Id, Rep2Id, Rep1Pct, Rep2Pct, Rep1CommRate, Rep2CommRate, CustPoNum, Subtotal, SubtotalPosted, SubtotalReleased, SubtotalPending, SubtotalRemaining, Freight, FreightPosted, FreightReleased, FreightPending, FreightRemaining, Misc, MiscPosted, MiscReleased, MiscPending, MiscRemaining, Total, TotalPosted, TotalReleased, TotalPending, TotalRemaining
*/

/*
-- tables currently used to retrieve data
tblSoSaleBlanket
tblSoSaleBlanketDetail
tblSoSaleBlanketDetailSch
tblSoSaleBlanketRelease
tblSoSaleBlanketReleaseDetail
*/

-- the following should come from the pick screen or be retrieved in report sp (not needed anywhere else on the report except to retrieve the data)
DECLARE 
@BaseCurrency pCurrency, 
@WksDate datetime, 
@CurrencyPrecision tinyint

SELECT @BaseCurrency = 'USD', @WksDate = '20101130', @CurrencyPrecision = 2

-- use a temp table to accumulate totals for each blanket
CREATE TABLE #Totals
(
	BlanketRef int NOT NULL, 
	TotalType tinyint NOT NULL, -- 0 = Ordered, 1 = Posted, 2 = Released, 3 = Pending
	SubTotal pDec DEFAULT(0), 
	Freight pDec DEFAULT(0), 
	Misc pDec DEFAULT(0)
)

-- use a temp table to capture currency information
CREATE TABLE #CurrencyInfo
(
	CurrencyId pCurrency, 
	PrecCurr tinyint, 
	ExchRate pDec
)

INSERT INTO #CurrencyInfo (CurrencyId, PrecCurr, ExchRate) 
SELECT CurrId, CurrDecPlaces, 1.0 
FROM dbo.glbSmCurrency WHERE (CurrId = @ReportCurrency) OR (@PrintAllInBaseCurrency = 1)

UPDATE #CurrencyInfo SET ExchRate = e.ExchRate 
FROM SYS.dbo.glbExchRate e 
	INNER JOIN (SELECT CurrencyTo, MAX(Counter) AS Counter 
				FROM SYS.dbo.glbExchRate 
				WHERE CurrencyFrom = @BaseCurrency AND EffectDate &lt;= @WksDate 
				GROUP BY CurrencyTo) t 
		ON e.CurrencyTo = t.CurrencyTo AND e.Counter = t.Counter 
WHERE #CurrencyInfo.CurrencyId = e.CurrencyTo

-- Capture the 'Ordered' values from the header
--  Dollar amount type blankets use a fixed ContractAmount
--   so offset it by the freight and misc to calculate the subtotal
INSERT INTO #Totals(BlanketRef, TotalType, Freight, Misc, SubTotal) 
SELECT BlanketRef, 0, Freight, Misc
	, CASE WHEN BlanketType = 0 THEN ContractAmount - Freight - Misc ELSE SubTotal END 
FROM dbo.tblSoSaleBlanket h 
WHERE ((h.CurrencyId = @ReportCurrency) OR (@PrintAllInBaseCurrency = 1)) -- single currency or all in base currency 
--	report filter
--	AND ((h.BlanketStatus = @BlanketStatus) OR (@BlanketStatus = -1)) 
--	AND ((h.BlanketType = @BlanketType) OR (@BlanketType = -1)) 
--	AND h.SoldToId BETWEEN @CustomerIdFrom AND @CustomerIdThru 
--	AND h.BlanketId BETWEEN @BlanketIdFrom AND @BlanketIdThru

-- calculate any 'Pending' values from the existing blanket details (no freight/misc on line items)
--  Scheduled Blankets - pull quantities as of the current workstation date
INSERT INTO #Totals(BlanketRef, TotalType, Freight, Misc, SubTotal) 
SELECT d.BlanketRef, 3, 0, 0, SUM(s.QtyOrdered * d.UnitPrice) 
FROM dbo.tblSoSaleBlanket h 
	INNER JOIN dbo.tblSoSaleBlanketDetail d ON h.BlanketRef = d.BlanketRef 
	INNER JOIN dbo.tblSoSaleBlanketDetailSch s ON d.BlanketDtlRef = s.BlanketDtlRef 
WHERE ((h.CurrencyId = @ReportCurrency) OR (@PrintAllInBaseCurrency = 1)) -- single currency or all in base currency
	AND s.QtyOrdered &lt;&gt; 0 AND s.Status = 0 AND s.ReleaseDate &lt;= @WksDate AND h.BlanketType = 2 -- scheduled only
	--	report filter
--	AND ((h.BlanketStatus = @BlanketStatus) OR (@BlanketStatus = -1)) 
--	AND ((h.BlanketType = @BlanketType) OR (@BlanketType = -1)) 
--	AND h.SoldToId BETWEEN @CustomerIdFrom AND @CustomerIdThru 
--	AND h.BlanketId BETWEEN @BlanketIdFrom AND @BlanketIdThru 
GROUP BY d.BlanketRef

-- calculate any 'Pending' values from the existing blanket details (no freight/misc on line items)
--  Non-Scheduled Blankets
INSERT INTO #Totals(BlanketRef, TotalType, Freight, Misc, SubTotal) 
SELECT d.BlanketRef, 3, 0, 0, SUM(d.QtyReleased * d.UnitPrice) 
FROM dbo.tblSoSaleBlanket h 
	INNER JOIN dbo.tblSoSaleBlanketDetail d ON h.BlanketRef = d.BlanketRef 
WHERE ((h.CurrencyId = @ReportCurrency) OR (@PrintAllInBaseCurrency = 1)) -- single currency or all in base currency
	AND d.QtyOrdered &lt;&gt; 0 AND h.BlanketType &lt;&gt; 2 -- non-scheduled only
--	report filter
--	AND ((h.BlanketStatus = @BlanketStatus) OR (@BlanketStatus = -1)) 
--	AND ((h.BlanketType = @BlanketType) OR (@BlanketType = -1)) 
--	AND h.SoldToId BETWEEN @CustomerIdFrom AND @CustomerIdThru 
--	AND h.BlanketId BETWEEN @BlanketIdFrom AND @BlanketIdThru 
GROUP BY d.BlanketRef

-- capture 'Posted' values for Freight and Misc from tblSoSaleBlanketRelease
INSERT INTO #Totals(BlanketRef, TotalType, Freight, Misc) 
SELECT h.BlanketRef, 1, SUM(SIGN(r.RecType) * r.Freight), SUM(SIGN(r.RecType) * r.Misc) 
FROM dbo.tblSoSaleBlanket h 
	INNER JOIN dbo.tblSoSaleBlanketRelease r ON h.BlanketRef = r.BlanketRef 
WHERE ((h.CurrencyId = @ReportCurrency) OR (@PrintAllInBaseCurrency = 1)) -- single currency or all in base currency
--	report filter
--	AND ((h.BlanketStatus = @BlanketStatus) OR (@BlanketStatus = -1)) 
--	AND ((h.BlanketType = @BlanketType) OR (@BlanketType = -1)) 
--	AND h.SoldToId BETWEEN @CustomerIdFrom AND @CustomerIdThru 
--	AND h.BlanketId BETWEEN @BlanketIdFrom AND @BlanketIdThru 
GROUP BY h.BlanketRef

-- capture 'Posted' and 'Released' Subtotal for 'Dollar Amount' blankets from tblSoSaleBlanketRelease
--  (non-dollar blankets will not have a 'release' record)
INSERT INTO #Totals(BlanketRef, TotalType, SubTotal) 
SELECT h.BlanketRef , CASE WHEN r.PostRun IS NULL THEN 2 ELSE 1 END , SUM(SIGN(r.RecType) * r.SubTotal) 
FROM dbo.tblSoSaleBlanket h 
	INNER JOIN dbo.tblSoSaleBlanketRelease r ON h.BlanketRef = r.BlanketRef 
WHERE ((h.CurrencyId = @ReportCurrency) OR (@PrintAllInBaseCurrency = 1)) -- single currency or all in base currency
	AND h.BlanketType = 0 -- amount type only
--	report filter
--	AND ((h.BlanketStatus = @BlanketStatus) OR (@BlanketStatus = -1)) 
--	AND ((h.BlanketType = @BlanketType) OR (@BlanketType = -1)) 
--	AND h.SoldToId BETWEEN @CustomerIdFrom AND @CustomerIdThru 
--	AND h.BlanketId BETWEEN @BlanketIdFrom AND @BlanketIdThru 
GROUP BY h.BlanketRef, r.PostRun

-- calculate 'Posted' and 'Released' Subtotal for NON-'Dollar Amount' blankets from tblSoSaleBlanketReleaseDetail
INSERT INTO #Totals(BlanketRef, TotalType, SubTotal) 
SELECT h.BlanketRef , CASE WHEN rd.PostRun is NULL THEN 2 ELSE 1 END, SUM(SIGN(rd.RecType) * rd.PriceExt) 
FROM dbo.tblSoSaleBlanket h 
	INNER JOIN dbo.tblSoSaleBlanketDetail d ON h.BlanketRef = d.BlanketRef 
	INNER JOIN dbo.tblSoSaleBlanketReleaseDetail rd ON d.BlanketDtlRef = rd.BlanketDtlRef 
WHERE ((h.CurrencyId = @ReportCurrency) OR (@PrintAllInBaseCurrency = 1)) -- single currency or all in base currency
	AND h.BlanketType &lt;&gt; 0 -- non-amount
--	report filter
--	AND ((h.BlanketStatus = @BlanketStatus) OR (@BlanketStatus = -1)) 
--	AND ((h.BlanketType = @BlanketType) OR (@BlanketType = -1)) 
--	AND h.SoldToId BETWEEN @CustomerIdFrom AND @CustomerIdThru 
--	AND h.BlanketId BETWEEN @BlanketIdFrom AND @BlanketIdThru 
GROUP BY h.BlanketRef, rd.PostRun

-- return a resultset of the values (include placeholders for freight &amp; misc)
SELECT CASE WHEN @SortBy = 0 THEN h.SoldToId ELSE h.BlanketId END AS SortBy
	, h.SoldToId, h.BlanketId, h.BlanketRef, h.BlanketType, h.BlanketStatus
	, h.ShipToId, h.CloseDate, h.ExpireDate, h.Rep1Id, h.Rep2Id, h.Rep1Pct, h.Rep2Pct, h.Rep1CommRate, h.Rep2CommRate
	, h.CustPoNum
	, CAST(CASE WHEN @PrintAllInBaseCurrency = 1 THEN ROUND(tot.Subtotal / c.ExchRate, CAST(@CurrencyPrecision AS int)) ELSE ROUND(tot.Subtotal, c.PrecCurr) END AS float) AS Subtotal
	, CAST(CASE WHEN @PrintAllInBaseCurrency = 1 THEN ROUND(tot.SubTotalPosted / c.ExchRate, CAST(@CurrencyPrecision AS int)) ELSE ROUND(tot.SubTotalPosted, c.PrecCurr)END AS float) AS SubtotalPosted
	, CAST(CASE WHEN @PrintAllInBaseCurrency = 1 THEN ROUND(tot.SubtotalReleased / c.ExchRate, CAST(@CurrencyPrecision AS int)) ELSE ROUND(tot.SubtotalReleased, c.PrecCurr)END AS float) AS SubtotalReleased
	, CAST(CASE WHEN @PrintAllInBaseCurrency = 1 THEN ROUND(tot.SubtotalPending / c.ExchRate, CAST(@CurrencyPrecision AS int)) ELSE ROUND(tot.SubtotalPending, c.PrecCurr)END AS float) AS SubtotalPending
	, CAST(CASE WHEN @PrintAllInBaseCurrency = 1 
		THEN ROUND((tot.Subtotal - CASE WHEN tot.SubTotalReleased &gt; tot.SubTotalPosted THEN tot.SubTotalReleased ELSE tot.SubTotalPosted END) / c.ExchRate, CAST(@CurrencyPrecision AS int))
		ELSE ROUND((tot.Subtotal - CASE WHEN tot.SubTotalReleased &gt; tot.SubTotalPosted THEN tot.SubTotalReleased ELSE tot.SubTotalPosted END), c.PrecCurr) 
		END AS float) AS SubtotalRemaining
	, CAST(CASE WHEN @PrintAllInBaseCurrency = 1 THEN ROUND(tot.Freight / c.ExchRate, CAST(@CurrencyPrecision AS int)) ELSE ROUND(tot.Freight, c.PrecCurr)END AS float) AS Freight
	, CAST(CASE WHEN @PrintAllInBaseCurrency = 1 THEN ROUND(tot.FreightPosted / c.ExchRate, CAST(@CurrencyPrecision AS int)) ELSE ROUND(tot.FreightPosted, c.PrecCurr)END AS float) AS FreightPosted
	, CAST(CASE WHEN @PrintAllInBaseCurrency = 1 THEN ROUND(tot.FreightReleased / c.ExchRate, CAST(@CurrencyPrecision AS int)) ELSE ROUND(tot.FreightReleased, c.PrecCurr)END AS float) AS FreightReleased
	, CAST(CASE WHEN @PrintAllInBaseCurrency = 1 THEN ROUND(tot.FreightPending / c.ExchRate, CAST(@CurrencyPrecision AS int)) ELSE ROUND(tot.FreightPending, c.PrecCurr)END AS float) AS FreightPending
	, CAST(CASE WHEN @PrintAllInBaseCurrency = 1 
		THEN ROUND((tot.Freight - CASE WHEN tot.FreightReleased &gt; tot.FreightPosted THEN tot.FreightReleased ELSE tot.FreightPosted END) / c.ExchRate, CAST(@CurrencyPrecision AS int)) 
		ELSE ROUND((tot.Freight - CASE WHEN tot.FreightReleased &gt; tot.FreightPosted THEN tot.FreightReleased ELSE tot.FreightPosted END) , c.PrecCurr)
		END AS float) AS FreightRemaining
	, CAST(CASE WHEN @PrintAllInBaseCurrency = 1 THEN ROUND(tot.Misc / c.ExchRate, CAST(@CurrencyPrecision AS int)) ELSE ROUND(tot.Misc, c.PrecCurr)END AS float) AS Misc
	, CAST(CASE WHEN @PrintAllInBaseCurrency = 1 THEN ROUND(tot.MiscPosted / c.ExchRate, CAST(@CurrencyPrecision AS int)) ELSE ROUND(tot.MiscPosted, c.PrecCurr)END AS float) AS MiscPosted
	, CAST(CASE WHEN @PrintAllInBaseCurrency = 1 THEN ROUND(tot.MiscReleased / c.ExchRate, CAST(@CurrencyPrecision AS int)) ELSE ROUND(tot.MiscReleased, c.PrecCurr)END AS float) AS MiscReleased
	, CAST(CASE WHEN @PrintAllInBaseCurrency = 1 THEN ROUND(tot.MiscPending / c.ExchRate, CAST(@CurrencyPrecision AS int)) ELSE ROUND(tot.MiscPending, c.PrecCurr)END AS float) AS MiscPending
	, CAST(CASE WHEN @PrintAllInBaseCurrency = 1 
		THEN ROUND((tot.Misc - CASE WHEN tot.MiscReleased &gt; tot.MiscPosted THEN tot.MiscReleased ELSE tot.MiscPosted END) / c.ExchRate, CAST(@CurrencyPrecision AS int))
		ELSE ROUND((tot.Misc - CASE WHEN tot.MiscReleased &gt; tot.MiscPosted THEN tot.MiscReleased ELSE tot.MiscPosted END), c.PrecCurr)
		END AS float) AS MiscRemaining
	, CAST(CASE WHEN @PrintAllInBaseCurrency = 1 THEN ROUND(tot.Total / c.ExchRate, CAST(@CurrencyPrecision AS int)) ELSE ROUND(tot.Total, c.PrecCurr)END AS float) AS Total
	, CAST(CASE WHEN @PrintAllInBaseCurrency = 1 THEN ROUND(tot.TotalPosted / c.ExchRate, CAST(@CurrencyPrecision AS int)) ELSE ROUND(tot.TotalPosted, c.PrecCurr)END AS float) AS TotalPosted
	, CAST(CASE WHEN @PrintAllInBaseCurrency = 1 THEN ROUND(tot.TotalReleased / c.ExchRate, CAST(@CurrencyPrecision AS int)) ELSE ROUND(tot.TotalReleased, c.PrecCurr)END AS float) AS TotalReleased
	, CAST(CASE WHEN @PrintAllInBaseCurrency = 1 THEN ROUND(tot.TotalPending / c.ExchRate, CAST(@CurrencyPrecision AS int)) ELSE ROUND(tot.TotalPending, c.PrecCurr)END AS float) AS TotalPending
	, CAST(CASE WHEN @PrintAllInBaseCurrency = 1 
		THEN ROUND((tot.Total - CASE WHEN tot.TotalReleased &gt; tot.TotalPosted THEN tot.TotalReleased ELSE tot.TotalPosted END) / c.ExchRate, CAST(@CurrencyPrecision AS int))
		ELSE ROUND((tot.Total - CASE WHEN tot.TotalReleased &gt; tot.TotalPosted THEN tot.TotalReleased ELSE tot.TotalPosted END), c.PrecCurr)
		END AS float) AS TotalRemaining 
FROM dbo.tblSoSaleBlanket h 
	INNER JOIN (SELECT BlanketRef
					, SUM(CASE WHEN TotalType = 0 THEN Subtotal ELSE 0 END) AS SubTotal 
					, SUM(CASE WHEN TotalType = 1 THEN SubTotal ELSE 0 END) AS SubTotalPosted
					, SUM(CASE WHEN TotalType = 2 THEN Subtotal ELSE 0 END) AS SubTotalReleased
					, SUM(CASE WHEN TotalType = 3 THEN Subtotal ELSE 0 END) AS SubTotalPending
					, SUM(CASE WHEN TotalType = 0 THEN Freight ELSE 0 END) AS Freight
					, SUM(CASE WHEN TotalType = 1 THEN Freight ELSE 0 END) AS FreightPosted 
					, SUM(CASE WHEN TotalType = 2 THEN Freight ELSE 0 END) AS FreightReleased
					, SUM(CASE WHEN TotalType = 3 THEN Freight ELSE 0 END) AS FreightPending
					, SUM(CASE WHEN TotalType = 0 THEN Misc ELSE 0 END) AS Misc
					, SUM(CASE WHEN TotalType = 1 THEN Misc ELSE 0 END) AS MiscPosted 
					, SUM(CASE WHEN TotalType = 2 THEN Misc ELSE 0 END) AS MiscReleased
					, SUM(CASE WHEN TotalType = 3 THEN Misc ELSE 0 END) AS MiscPending
					, SUM(CASE WHEN TotalType = 0 THEN Subtotal + Freight + Misc ELSE 0 END) AS Total
					, SUM(CASE WHEN TotalType = 1 THEN Subtotal + Freight + Misc ELSE 0 END) AS TotalPosted
					, SUM(CASE WHEN TotalType = 2 THEN Subtotal + Freight + Misc ELSE 0 END) AS TotalReleased
					, SUM(CASE WHEN TotalType = 3 THEN Subtotal + Freight + Misc ELSE 0 END) AS TotalPending 
				FROM #Totals 
				GROUP BY BlanketRef) tot 
		ON h.BlanketRef = tot.BlanketRef 
	LEFT JOIN #CurrencyInfo c ON h.CurrencyId = c.CurrencyId</CommandText>
        <QueryParameters>
          <QueryParameter Name="@ReportCurrency">
            <Value>=Parameters!ReportCurrency.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@PrintAllInBaseCurrency">
            <Value>=Parameters!PrintAllInBaseCurrency.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@SortBy">
            <Value>=Parameters!SortBy.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
